{
  "name": "datoun-fe-2",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "prettier": "prettier --write {src,test}/*.{js,vue,ts,json} {src,test}/**/*.{js,vue,ts,json} --ignore-unknown --no-error-on-unmatched-pattern",
    "prepare": "husky install"
  },
  "dependencies": {
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vue-tsc": "^2.0.6",
    "lint-staged": "^15.2.2",
    "husky": "^8.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "{src,test}/**/*.{js,vue,ts,json}": [
      "prettier --write --ignore-unknown --no-error-on-unmatched-pattern"
    ]
  }
}
